{
  "name": "stock-watch",
  "version": "1.0.0",
  "main": "index.bundle.js",
  "scripts": {
    "watch": "webpack -d --watch",
    "start": "node ./index.bundle.js",
    "dev": "nodemon ./index.bundle.js",
    "db:migrate": "yarn sequelize db:migrate --config ./db/config/config.json --migrations-path ./db/migrations --models-path ./db/models",
    "create:migration": "yarn sequelize migration:generate --config ./db/config/config.json --migrations-path ./db/migrations --models-path ./db/models",
    "db:migrate:undo": "yarn sequelize db:migrate:undo --config ./db/config/config.json --migrations-path ./db/migrations --models-path ./db/models",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "http://www.github.com/idahogurl/stock-watch"
  },
  "author": "Rebecca Vest",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dayjs": "^1.7.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-tag-pluck": "^0.8.6",
    "graphql-tools": "^3.0.2",
    "graphql-tools-types": "^1.1.21",
    "lodash": "^4.17.15",
    "merge-graphql-schemas": "^1.5.1",
    "pg": "^7.4.3",
    "rollbar": "^2.14.0",
    "sequelize": "^5.21.0",
    "sequelize-cli": "^4.0.0",
    "sequelize-heroku": "^2.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.0",
    "concurrently": "^3.5.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.4",
    "sqlite3": "^4.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  }
}
